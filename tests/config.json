{
    "test_settings": {
        "default": {
            "symbol": "EURUSD",
            "timeframe": "PERIOD_H1",
            "start_date": "2023.01.01",
            "end_date": "2023.12.31",
            "initial_deposit": 10000,
            "currency": "USD"
        },
        "pattern_recognition": {
            "min_pattern_bars": 10,
            "max_pattern_bars": 100,
            "confidence_threshold": 75.0,
            "test_patterns": [
                "FLAG",
                "PENNANT",
                "CHANNEL",
                "TRIANGLE",
                "HEAD_SHOULDERS",
                "DOUBLE_TOP_BOTTOM",
                "TRIPLE_TOP_BOTTOM",
                "ROUNDING_BOTTOM",
                "CUP_HANDLE"
            ]
        },
        "zone_detection": {
            "min_zone_bars": 5,
            "zone_strength": 70.0,
            "zone_depth_factor": 2.0,
            "max_zone_age": 500,
            "merge_distance": 0.0010
        },
        "fibonacci_analysis": {
            "levels": [
                -0.618,
                0.0,
                0.5,
                0.618,
                1.0,
                1.618
            ],
            "zone_highlight": true,
            "zone_opacity": 0.2,
            "swing_lookback": 20,
            "level_tolerance": 0.0010
        },
        "signal_generation": {
            "min_signal_strength": 70.0,
            "min_confluence": 3,
            "min_rr_ratio": 1.5,
            "atr_period": 14,
            "sl_multiplier": 2.0,
            "tp_multiplier": 3.0
        }
    },
    "test_cases": {
        "pattern_recognition_tests": [
            {
                "name": "Flag Pattern Detection",
                "pattern_type": "FLAG",
                "data_file": "test_data/flag_pattern.csv",
                "expected_results": {
                    "pattern_found": true,
                    "confidence": 80.0,
                    "is_bullish": true
                }
            },
            {
                "name": "Channel Pattern Detection",
                "pattern_type": "CHANNEL",
                "data_file": "test_data/channel_pattern.csv",
                "expected_results": {
                    "pattern_found": true,
                    "confidence": 85.0,
                    "channel_type": "ASCENDING"
                }
            }
        ],
        "zone_detection_tests": [
            {
                "name": "Supply Zone Detection",
                "data_file": "test_data/supply_zone.csv",
                "expected_results": {
                    "zone_found": true,
                    "zone_strength": 75.0,
                    "is_supply": true
                }
            },
            {
                "name": "Demand Zone Detection",
                "data_file": "test_data/demand_zone.csv",
                "expected_results": {
                    "zone_found": true,
                    "zone_strength": 80.0,
                    "is_supply": false
                }
            }
        ],
        "fibonacci_tests": [
            {
                "name": "Fibonacci Level Calculation",
                "data_file": "test_data/fibonacci_test.csv",
                "expected_results": {
                    "swing_high": 1.2000,
                    "swing_low": 1.1800,
                    "is_uptrend": true,
                    "level_count": 6
                }
            }
        ],
        "signal_generation_tests": [
            {
                "name": "Buy Signal Generation",
                "data_file": "test_data/buy_signal.csv",
                "expected_results": {
                    "signal_type": "STRONG_BUY",
                    "signal_strength": 85.0,
                    "confluence_count": 4
                }
            },
            {
                "name": "Sell Signal Generation",
                "data_file": "test_data/sell_signal.csv",
                "expected_results": {
                    "signal_type": "STRONG_SELL",
                    "signal_strength": 82.0,
                    "confluence_count": 3
                }
            }
        ]
    },
    "performance_tests": {
        "pattern_recognition": {
            "min_accuracy": 85.0,
            "max_false_positives": 15.0,
            "min_confidence": 75.0
        },
        "zone_detection": {
            "min_accuracy": 80.0,
            "max_false_zones": 20.0,
            "min_strength": 70.0
        },
        "signal_generation": {
            "min_win_rate": 60.0,
            "min_profit_factor": 1.5,
            "max_drawdown": 20.0
        }
    },
    "test_data_generation": {
